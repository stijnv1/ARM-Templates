{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "vNetName": {
      "type": "string",
      "defaultValue": "VNET01",
      "metadata": {
        "Description": "Specify a VNET name"
      }
    },

    "vNetAddressPrefix": {
      "type": "string",
      "defaultValue": "10.10.10.0/24",
      "metadata": {
        "Description": "Address prefix"
      }
    },

    "Location": {
      "type": "string",
      "metadata": {
        "Description": "Deployment Location"
      }
    },

    "vNetSubnetBackEndPrefix": {
      "type": "string",
      "metadata": {
        "Description": "Subnet Prefix"
      }
    },

    "vNetSubnetBackEndName": {
      "type": "string",
      "metadata": {
        "Description": "Subnet Backend Name"
      }
    },

    "vNetSubnetFrontEndPrefix": {
      "type": "string",
      "metadata": {
        "Description": "Subnet Prefix"
      }
    },

    "vNetSubnetFrontEndName": {
      "type": "string",
      "metadata": {
        "Description": "Subnet Frontend Name"
      }
    }
  },

  "variables": {
    "apiVersion": {
      "vNets": {
        "VirtualNetworks": "2015-06-15"
      }
    }
  },

  "resources": [
    {
      "apiVersion": "[variables('apiVersion').vNets.VirtualNetworks]",
      "location": "[parameters('Location')]",
      "name": "[parameters('vNetName')]",
      "type": "Microsoft.Network/virtualNetworks",
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[parameters('vNetAddressPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[parameters('vNetSubnetBackEndName')]",
            "properties": {
              "addressPrefix": "[parameters('vNetSubnetBackEndPrefix')]"
            }
          },

          {
            "name": "[parameters('vNetSubnetFrontEndName')]",
            "properties": {
              "addressPrefix": "[parameters('vNetSubnetFrontEndPrefix')]"
            }
          }
        ]
      }
    }
  ],

  "outputs": {

  }
}
